{
    "name": "skybolt/skybolt-php",
    "version": "3.1.0",
    "description": "High-performance asset caching for multi-page applications",
    "type": "library",
    "authors": [
        {
            "name": "Jens Roland",
            "email": "jens@jensroland.com"
        }
    ],
    "license": "MIT",
    "keywords": [
        "assets",
        "caching",
        "service-worker",
        "performance",
        "vite"
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Skybolt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skybolt\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse src --level=8"
    },
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/JensRoland/skybolt-php/issues",
        "source": "https://github.com/JensRoland/skybolt-php"
    }
}
